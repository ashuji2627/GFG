class Solution {
  public:
    int maxSumIS(vector<int>& arr) {
        // code here
        vector<int>lis=arr;
    
        for(int i=0; i<arr.size(); i++) {
            for(int j=i-1; j>=0; j--) {
                if(arr[i] > arr[j]){
                    lis[i] = max(lis[i], lis[j]+arr[i]);
                }
            }
        }
        return *max_element(lis.begin() , lis.end());
    }
};
